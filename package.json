{
  "name": "promise-any",
  "version": "0.1.0",
  "description": "returns first successful promise. As in Promise.any() which is not part of the ES6 spec",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src test",
    "prebuild": "rimraf lib",
    "build": "babel src -d lib",
    "pretest": "rimraf coverage",
    "test": "NODE_ENV=test nyc --check-coverage --lines 100 ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "lint-staged",
    "prepush": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m0ppers/promise-any.git"
  },
  "keywords": [
    "promise",
    "es6",
    "any",
    "resolve"
  ],
  "author": "Andreas Streichardt <andreas.streichardt@gmail.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/m0ppers/promise-any/issues"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "homepage": "https://github.com/m0ppers/promise-any#readme",
  "devDependencies": {
    "@sadorlovsky/eslint-config": "^0.12.0",
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "coveralls": "^2.11.12",
    "eslint": "^3.3.1",
    "eslint-plugin-ava": "^2.5.0",
    "eslint-plugin-fp": "^1.3.0",
    "eslint-plugin-import": "^1.13.0",
    "husky": "^0.11.6",
    "lint-staged": "^2.0.3",
    "nyc": "^8.1.0",
    "rimraf": "^2.5.4"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "lint-staged": {
    "*.js": "eslint"
  }
}
